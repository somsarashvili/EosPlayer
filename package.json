{
  "name": "eos-player",
  "version": "0.0.29",
  "description": "EosPlayer",
  "main": "app/electron/main.js",
  "private": true,
  "scripts": {
    "generateapi": "api-client-generator -s ./swagger.json -o ./src/app/api/eos",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm run postinstall && npm-run-all -p ng:serve electron:serve",
    "preparebuild": "npm run postinstall && npm run electron:serve-tsc && ng build",
    "prebuild": "npm run preparebuild -- -c production",
    "build": "npm run prebuild",
    "build:dev": "npm run prebuild -- -c dev",
    "build:prod": "npm run prebuild -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall && ng serve -o",
    "electron:routes": "node tools/generateRoutes.js",
    "electron:serve-tsc": "tsc -p src/electron/tsconfig.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "npm run postinstall && ng test",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular/animations": "~14.2.9",
    "@angular/cli": "~14.2.8",
    "@angular/common": "~14.2.9",
    "@angular/compiler": "~14.2.9",
    "@angular/compiler-cli": "~14.2.9",
    "@angular/core": "~14.2.9",
    "@angular/forms": "~14.2.9",
    "@angular/platform-browser": "~14.2.9",
    "@angular/platform-browser-dynamic": "~14.2.9",
    "@angular/router": "~14.2.9",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@ngx-loading-bar/core": "^6.0.2",
    "@ngx-loading-bar/http-client": "^6.0.2",
    "@ngx-loading-bar/router": "^6.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.11.9",
    "bootstrap": "^5.2.2",
    "codelyzer": "^6.0.2",
    "electron": "21.2.1",
    "electron-builder": "23.6.0",
    "electron-reload": "1.5.0",
    "handlebars": "^4.7.7",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ngx-infinite-scroll": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs": "~7.5.7",
    "ts-node": "~10.9.1",
    "tslib": "^2.4.1",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4",
    "wait-on": "^6.0.1",
    "zone.js": "~0.11.8"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@types/webtorrent": "^0.109.3",
    "custom-electron-titlebar": "^4.1.0",
    "electron-log": "4.4.8",
    "electron-updater": "5.3.0",
    "inversify": "6.0.1",
    "reflect-metadata": "0.1.13",
    "v8-compile-cache": "2.3.0",
    "vlc-command": "^1.2.0",
    "webtorrent": "1.9.1",
    "webtorrent-health": "1.2.0"
  }
}
